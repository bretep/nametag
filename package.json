  "scripts": {
    "test": "jest",
    "test-watch": "jest --watch",
    "start": "docker-compose up -d && gulp watch",
    "build": "NODE_ENV=production gulp build",
    "update": "git pull origin master && npm install && npm run build && docker-compose build horizon --no-cache && docker-compose stop && docker-compose up -d",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest": {
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react"
    ],
    "testPathIgnorePatterns": [
      "/app/bower_components/",
      "/dist/bower_components",
      "/node_modules/",
      "/dist/",
      "/horizon/"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/app/scripts/tests/mockGlobals.js",
    "automock": true
  },
  "browserify": {
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "ignore": [
            "bower_components"
          ]
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./app/bower_components/jquery/dist/jquery.js"
